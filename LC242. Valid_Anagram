# LC242 Valid Anagram

# Sol 1 Using Counter 
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        d1 = Counter(s)
        d2 = Counter(t)
        return d1 == d2

# Sol 2 Default dict
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        d1 = collections.defaultdict(int)
        d2 = collections.defaultdict(int)
        for ch in s:
            d1[ch] += 1
        for ch in t:
            d2[ch] += 1
        return d1 == d2

# Sol 3 Default dict
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        d1 = collections.defaultdict(int)
        for ch in s:
            d1[ch] += 1
        for ch in t:
            if d1[ch] <= 0:
                return False
            else:
                d1[ch] -= 1
        return True



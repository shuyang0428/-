# LC283 Move Zeroes

# Sol 1 Time:O(n)
class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        n = 0
        for i in range(len(nums)):
            if nums[i] == 0:
                n += 1
        
        slow,fast = 0,0
        while fast < len(nums):
            if nums[fast] != 0:
                nums[slow] = nums[fast]
                slow += 1
            fast += 1
        j = 0    
        while n > 0 and j in range(len(nums)):
            nums[len(nums) - 1 - j] = 0
            n -= 1
            j += 1

# Sol 2
class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """

        pos = 0
        for i in range(len(nums)):
            if nums[i] != 0:
                nums[pos] = nums[i]
                pos += 1
        
        for i in range(pos,len(nums)):
            nums[i] = 0

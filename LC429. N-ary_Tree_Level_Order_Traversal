# LC429 N-ary Tree Level Order Traversal

class Solution:
    def levelOrder(self, root: 'Node') -> List[List[int]]:
        if not root:
            return []
        que = collections.deque([root])
        res = []

        while que:
            n = len(que)
            vec = []
            for _ in range(n):
                curr = que.popleft()
                vec.append(curr.val)
                if curr.children:
                    for child in curr.children:
                        que.append(child)
            res.append(vec)
        return res

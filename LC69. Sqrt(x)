# LC69 Sqrt(x)
class Solution:
    def mySqrt(self, x: int) -> int:
        if x < 2:
            return x
        left,right = 0,x // 2
        while left <= right:
            mid = left + (right - left) // 2
            if mid ** 2 > x:
                right = mid - 1
            elif mid ** 2 < x:
                left = mid + 1
            else:
                return mid
        return right
        

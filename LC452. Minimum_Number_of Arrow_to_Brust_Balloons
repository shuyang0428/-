# LC452 Minumum Number of Arrows to Brust Balloons

# Sol Greedy
class Solution:
    def findMinArrowShots(self, points: List[List[int]]) -> int:
        if not points:
            return 0

        points.sort(key = lambda x: x[1])

        res = 1
        right = points[0][1]
        for i in range(1,len(points)):
            if points[i][0] > right:
                res += 1
                right = points[i][1]
            else:
                continue

        return res

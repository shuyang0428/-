# LC350 Intersection of Two Arrays II

from collections import Counter 
class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        l = list()
        s = list()
        ans = []
        if len(nums1) >= len(nums2):
            l = nums1
            s = nums2
        else:
            l = nums2
            s = nums1
        
        d = Counter(s)

        for num in l:
            if d[num] > 0:
                ans.append(num)
                d[num] -= 1
        
        return ans

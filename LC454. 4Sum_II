# LC454 4Sum II

class Solution:
    def fourSumCount(self, nums1: List[int], nums2: List[int], nums3: List[int], nums4: List[int]) -> int:
        ans = 0
        sumdict = collections.defaultdict(int)

        for x in nums1:
            for y in nums2:
                sumdict[x+y] += 1
        
        for m in nums3:
            for n in nums4:
                remain = -(m+n)
                if remain in sumdict:
                    ans += sumdict[remain]
        return ans
